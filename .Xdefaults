URxvt.scrollBar: false
URxvt*termName: rxvt-256color

*.foreground: #c5c8c6
*.background: [10]#1d1f21
*.cursorColor: #DA70D6

! Black
*.color0: #282a2e
*.color8: #373b41

! red
*.color1: #a54242
*.color9: #cc6666

! green
*.color2: #8c9440
*.color10: #b5bd68

! yellow
*.color3: #de935f
*.color11: #f0c674

! blue
*.color4: #5f819d
*.color12: #81a2be

! magenta
*.color5: #85678f
*.color13: #b294bb

! cyan
*.color6: #5e8d87
*.color14: #8abeb7

! white
*.color7: #707880
*.color15: #c5c8c6
~
URxvt.perl-ext-common: tabbed


! Fonts
urxvt*font: xft:Fira Code:size=13,style=medium,\
            xft:Noto Color Emoji:size=10,\
            xft:DejaVu Sans Mono for Powerline:size=10,\
            xft:xft:TerminessTTFNerdFontMono:size=12
urxvt*boldFont: xft:Fira Code:size=13,style=bold
urxvt*italicFont: xft:Fira Code:size=13,style=bold
urxvt*boldItalicFont: xft:Fira Code:size=13,style=bold

! Transparency
urxvt*transparent: true
urxvt*shading: 15
! urxvt*blurRadius: 10
urxvt.inheritPixmap: true

Xft.dpi:                        96
Xft.antialias:                  1
Xft.autohint:                   1
Xft.hinting:                    1
Xft.hintstyle:                  hintslight
Xft.lcdfilter:                  lcdlight
Xft.rgba:                       rgb

URxvt.depth:                    32
URxvt.letterSpace:              -1
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt*loginShell:               true
URxvt.internalBorder:           5
URxvt.perl-ext:	 	            	default,matcher,resize-font
URxvt.url-launcher:             /usr/bin/xdg-open
Rxvt.matcher.button:            1
URxvt.matcher.rend.0:           Uline Bold fg5

! Keybindings
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Control-Down:  resize-font:smaller
URxvt.keysym.Control-Up:    resize-font:bigger

URxvt.scrollBar: false
URxvt.termName: rxvt-unicode-256color

*.foreground: #c5c8c6
*.background: [90]#1d1f21
*.cursorColor: #c5c8c6

! Black
*.color0: #282a2e
*.color8: #373b41

! red
*.color1: #a54242
*.color9: #cc6666

! green
*.color2: #8c9440
*.color10: #b5bd68

! yellow
*.color3: #de935f
*.color11: #f0c674

! blue
*.color4: #5f819d
*.color12: #81a2be

! magenta
*.color5: #85678f
*.color13: #b294bb

! cyan
*.color6: #5e8d87
*.color14: #8abeb7

! white
*.color7: #707880
*.color15: #c5c8c6
~
URxvt.perl-ext-common: tabbed


! Fonts
URxvt.font: xft:Fira Mono:size=11,style=medium,\
            xft:Noto Color Emoji,\
            xft:Font Awesome:pixelsize=27:minspace=False,\
            xft:PowerlineSymbols:pixelsize=27:minispace=False
URxvt.boldFont: xft:Fira Mono:size=11,style=bold
URxvt.italicFont: xft:Fira Mono:size=11,style=bold
URxvt.boldItalicFont: xft:Fira Mono:size=11,style=bold

URxvt.font: xft:FuraCode Nerd Font:style=Regular:size=11
URxvt.boldFont: xft:FuraCode Nerd Font:style=Regular:size=11

! Transparency
urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/c800
urxvt*transparent: true
urxvt*shading: 15

! Keybindings
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
